pico-8 cartridge // http://www.pico-8.com
version 11
__lua__
-- 1. character select screen
-- 2. ship select screen
-- 3. resource select screen
-- ... launch?
-- 4. asteriod attack
-- 5. internal events?

-- player's unit speed
player_spd = 0.2

bullet_spd = 2
bullet_r = 1

asteroid_spd = 1

header_h = 7

playable_x_min = 1
playable_y_min = header_h+1
playable_x_max = 126
playable_y_max = 126

function reset()
 player = {}
 player.x = 10
 player.y = 64
 player.spd_x = 0
 player.spd_y = 0
 bullets = {}
 asteroids = {}
 player_is_alive = true
 explosion_stage = 3
end

function _init()
 start_game()
end

function start_game()
 reset()
 mode = "game"
end

function _update60()
 if mode == "game" then
  update_game()
 elseif mode == "gameover" then
  update_gameover()
 end 
end

function update_game()
 update_player()
 update_bullets()
 update_asteroids()
end

function update_gameover()
 if (btnp(5)) then start_game() end
end

function update_player()
 local button_pressed = false
 if(btn(1)) then
  player.spd_x += player_spd
  button_pressed = true
 elseif (btn(0)) then
  player.spd_x -= player_spd
  button_pressed = true
 elseif (btn(2)) then 
  player.spd_y -= player_spd
  button_pressed = true
 elseif (btn(3)) then
  player.spd_y += player_spd
  button_pressed = true
 end 
 
 if not(button_pressed) then
  player.spd_x /= 1.2
  player.spd_y /= 1.2
 end
 
 player.x = mid(2,player.x+player.spd_x,playable_x_max-8)
 player.y = mid(9,player.y+player.spd_y,playable_y_max-8) 
end

function update_bullets()
 if (btnp(4)) then
  local bullet = {}
  bullet.x = player.x + 8
  bullet.y = player.y + 4
  add(bullets, bullet)
  sfx(1)
 end
 
 for item in all(bullets) do
  item.x += bullet_spd
  if item.x > playable_x_max then
   del(bullets, item)
  end
 end
end

function update_asteroids()
 if flr(rnd(10)%10) == 1 then
  local asteroid = {}
  asteroid.x = playable_x_max
  asteroid.y = playable_y_min + flr(rnd(playable_y_max-8-playable_y_min))
  
  if not(ast_overlap(asteroid.y)) then
   add(asteroids, asteroid)
  end
 end 
 
 for item in all(asteroids) do
  item.x -= asteroid_spd
  if item.x < -7 then
   del(asteroids, item)
  elseif hit_by_bullet(item) then
   del(asteroids, item)
   sfx(2)
  elseif hit_player(item) then
   sfx(3)
   player_is_alive = false  
  end
 end
end

function _draw()
 if mode == "game" then 
  draw_game()
 elseif mode == "gameover" then
  draw_gameover()
 end
end

function draw_game()
 cls()
 draw_background()
 draw_header()
 draw_bullets()
 draw_asteroids()
 draw_player()
end

function draw_gameover()
 rectfill(0,60,128,75,0)
 print("game over",46,62,7)
 print("press — to restart",27,68,6)
end

function draw_background()
 rect(0, 0, 127, 127, 7)
end

function draw_header()
 rectfill(0, 0, 127, header_h, 6)
end

function draw_player()
 if player_is_alive then
  spr(1,player.x, player.y)
 else
  spr(explosion_stage, player.x, player.y)
  explosion_stage += 1
  if explosion_stage > 6 then
  	mode = "gameover"
 	end  
 end
end

function draw_bullets()
 for item in all(bullets) do
  circfill(item.x, item.y, bullet_r, 10)
 end
end

function draw_asteroids()
 for item in all(asteroids) do
  spr(2,item.x, item.y)
 end
end

-- helpers
function ast_overlap(_y)
 for item in all(asteroids) do
  if item.x > playable_x_max - 8 then
   if abs(item.y-_y) < 8 then
    return true
   end
  end 
 end
 return false
end

function hit_by_bullet(_ast)
 for bullet in all(bullets) do
  if ball_box(bullet.x, bullet.y, _ast.x, _ast.y) then
   return true
  end
 end
 return false
end

function hit_player(_ast)
 return box_box(player.x, player.y, _ast.x, _ast.y)
end

function ball_box(bx,by,box_x,box_y)
 if by-bullet_r > box_y+8 then return false end
 if by+bullet_r < box_y then return false end
 if bx-bullet_r > box_x+8 then return false end
 if bx+bullet_r < box_x then return false end
 return true
end

function box_box(box1_x,box1_y,box2_x,box2_y)
 if box1_y > box2_y+8 then return false end
 if box1_y+8 < box2_y then return false end
 if box1_x > box2_x+8 then return false end
 if box1_x+8 < box2_x then return false end
 return true
end
